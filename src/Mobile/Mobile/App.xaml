<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="DarkPrimaryColor">#7B1FA2</Color>
            <Color x:Key="LightPrimaryColor">#E1BEE7</Color>
            <Color x:Key="PrimaryColor">#9C27B0</Color>
            <Color x:Key="TextIcons">#FFFFFF</Color>
            <Color x:Key="AccentColor">#9E9E9E</Color>
            <Color x:Key="PrimaryText">#212121</Color>
            <Color x:Key="SecondaryText">#757575</Color>
            <Color x:Key="DividerColor">#BDBDBD</Color>
            <Color x:Key="BackgroundColor">White</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>